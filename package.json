{
  "name": "@owlly/e-collecting",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/e-collecting/e-collecting.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --silent",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run build && build-storybook -s dist/ -o docs/"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@storybook/addon-knobs": "^6.0.22"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/html": "^6.0.21",
    "@types/jest": "^26.0.12",
    "@types/puppeteer": "^3.0.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "prettier": "2.1.1",
    "pretty-quick": "^3.0.1",
    "puppeteer": "^5.2.1",
    "react-is": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
